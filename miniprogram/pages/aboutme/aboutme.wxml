<!--pages/aboutme/aboutme.wxml-->



<view class = "backgrd" style="background-image:url(data:image/jpeg;base64,);">
<view class = 'head'>
      <image class = "head-img" src = "../../images/icon/head.png"></image>
      <view class="text-black">
              <text class="text-cut">姓名：{{userInfo.data[0].name}}</text>
      </view>
      <view class="text-black">
              <text class="text-cut">ID：{{userInfo.data[0].studentId}}</text>
      </view>
</view>
</view>
  
<view wx:if="{{student == true}}">
    <view class="cu-list grid col-{{gridCol}} {{gridBorder?'':'no-border'}}">
    <view class="cu-item" bindtap='historyCheck'>
      <view class="icon-global text-red"></view>
      <text>学习记录</text>
    </view>

    <view class="cu-item" >
      <view class="icon-male text-red"></view>
      <text>性别-男</text>
    </view>

    <view class="cu-item">
      <view class="icon-discover text-purple"></view>
      <text>年级-15级</text>
    </view>

    <view class="cu-item">
      <view class="icon-read text-pink"></view>
      <text>专业-计算机科学与技术</text>
    </view>

    <view class="cu-item" bindtap='quit'>
      <view class="icon-link text-red"></view>
      <text>退出登录</text>
    </view>
  </view>
</view>



<view wx:if="{{student==false}}">
    <view class="cu-list grid col-{{gridCol}} {{gridBorder?'':'no-border'}}">
    <view class="cu-item" >
      <view class="icon-male text-red"></view>
      <text>性别-男</text>
    </view>

    <view class="cu-item" bindtap = "uploadChoose">
      <view class="icon-upload text-purple"></view>
      <text>上传课程/课件</text>
    </view>


    <view class="cu-item" bindtap = "manageChoose">
      <view class="icon-edit text-pink"></view>
      <text>管理我的发布</text>
    </view>

    <view class="cu-item" bindtap = "uploadInfo">
      <view class="icon-info text-yellow"></view>
      <text>发布通知</text>
    </view>

    

    

    <view class="cu-item" bindtap='quit'>
      <view class="icon-link text-red"></view>
      <text>退出登录</text>
    </view>
  </view>
</view>

<i-modal title="选择上传类型" visible="{{ visible4 }}" actions="{{ actions4 }}" action-mode="{{ vertical }}" bind:click="handleClick4" >
</i-modal>


<i-modal title="管理我的" visible="{{ visible5 }}" actions="{{ actions5 }}" action-mode="{{ vertical }}" bind:click="handleClick5" >
</i-modal>
